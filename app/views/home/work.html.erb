    <div class="newsletter-signup-container">
        <section class="module newsletter-signup newsletter-signup-dark" data-visible="false" data-loadmodule="NewsletterSignupForm">
            <div class="module-background"></div>
            <div class="module-content">
                <div class="newsletter-signup-content">
                    <header class="module-header">
                        <h2 class="headline">Stay current. Get our newsletter.</h2>
                    </header>
                    <form action="/services/prospect/newsletter/subscribe" class="form" method="post">
                        <div class="form-field">
                            <input type="email" id="email" name="email" data-request-key="email" value="" class="form-input" autocomplete="off" required pattern="^[a-zA-Z0-9.!#$%&‚Äô*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)+$" title="Enter a valid email address">
                            <label for="email" class="form-label" data-error="Enter a valid email address">Enter your email</label>
                        </div>
                        <div class="form-field">
                            <input type="text" id="firstname" name="firstname" data-request-key="firstName" value="" class="form-input" autocomplete="off" required maxlength="40">
                            <label for="firstname" class="form-label" data-error="Enter a valid First Name">First Name</label>
                        </div>
                        <div class="form-field">
                            <input type="text" id="lastname" name="lastname" data-request-key="lastName" value="" class="form-input" autocomplete="off" required maxlength="80">
                            <label for="lastname" class="form-label" data-error="Enter a valid Last Name">Last Name</label>
                        </div>
                        <div class="form-field">
                            <input type="text" id="company" name="company" data-request-key="company" value="" class="form-input" autocomplete="off" required maxlength="200">
                            <label for="company" class="form-label" data-error="Enter a valid Company Name">Company</label>
                        </div>

                        <!-- Hidden field -->
                        <input type="hidden" id="webCampaign" name="webCampaign" value="Razorfish Newsletter Sign Up" data-request-key="webCampaign">
                        <!-- / Hidden field -->

                        <div class="form-field form-field-submit">
                            <button type="submit" class="form-submit cta">
                                <span class="label">Subscribe</span>
                                <span class="spinner"></span>
                            </button>
                        </div>
                        <div class="form-field form-field-note">
                            <span class="form-note">* All fields required</span>
                        </div>
                        <div class="form-field form-field-error hidden">
                            <p class="form-error hidden general-error">We had a problem connecting to the server. Wait just a moment and try submitting again</p>
                            <p class="form-error hidden email-error">Enter a valid Email Address</p>
                        </div>
                    </form>
                </div>
            </div>
        </section>
        <section class="module newsletter-signup-success newsletter-signup-dark hidden" data-loadmodule="NewsletterSignupFormSuccess" id="newsletter-signup-success">
            <div class="module-background"></div>
            <div class="module-content">
                <header class="module-header">
                    <h2 class="headline">Congratulations!</h2>
                </header>
                <p class="form-text">Your form has been submitted.</p>
            </div>
        </section>
    </div>

